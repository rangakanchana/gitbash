package com.main;

public class PrimeCheck {
	public static void main(String[] args) {
	int n = 25;
	for(int i =2;i<=n/2;i++){
	if(n%i==1){
		System.out.println("the given number is " +n+ "not a prime number");
	}
	else{
		System.out.println("the given numer " + n+ "is a prime number");
	}
		
	}
	
		
	}
	
	
	
	
	
	
	 }




