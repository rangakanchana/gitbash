package com.main;

public class PrimeNoCheck {
	public static void main(String[] args) {

	Boolean isprime = true;
	int n = 25;
		for(int i =2;i<+n/2;i++){
		if(n%i==0){
			isprime=false;
			break;
			}
			}
		if(isprime){
			System.out.println(n+ "the number is a prime number");
			}else{
				System.out.println(n+ "the number is not a prime number");
			}
		
	}
	
	
	
		
}